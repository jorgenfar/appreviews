{
  "extends": ["airbnb-base", "prettier", "prettier/react"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "max-len": ["error", 100],
    "import/no-dynamic-require": "off",
    "global-require": "off",
    "no-underscore-dangle": "off",
    "no-console": "off",
    "import/prefer-default-export": "off"
  },
  "env": {
    "node": true
  }
}
